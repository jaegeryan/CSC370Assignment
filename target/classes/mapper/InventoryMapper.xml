<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uvic.mapper.InventoryMapper">

    <resultMap id="BaseResultMap" type="com.uvic.entity.Inventory">
            <id property="inventoryId" column="inventory_id" jdbcType="INTEGER"/>
            <result property="consoleId" column="console_id" jdbcType="INTEGER"/>
            <result property="serialNumber" column="serial_number" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="OTHER"/>
    </resultMap>

    <sql id="Base_Column_List">
        inventory_id,console_id,serial_number,
        status
    </sql>
</mapper>
